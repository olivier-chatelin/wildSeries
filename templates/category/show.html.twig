{# templates/program/show.html.twig #}

{% extends 'base.html.twig' %}
{% block title %}Séries de la catégorie {% endblock %}
{% block body %}
    {% for program in programs %}
        <div>
            <h2>Série # {{ program.id }} <a href="{{ path('program_show',{'id': program.id}) }} ">{{ program.title }}</a></h2>
            <p>{{ program.summary }}</p>
        </div>
        {% else %}
        <p>Aucune série trouvée</p>
    {% endfor %}
    <div>

    </div>
{% endblock %}
